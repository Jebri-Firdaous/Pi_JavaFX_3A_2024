      <Text fx:id="year" strokeType="OUTSIDE" strokeWidth="0.0" text="####">
         <font>
            <Font size="22.0"/>
         </font>
      </Text>
      <Text fx:id="month" strokeType="OUTSIDE" strokeWidth="0.0" text="####">
         <font>
            <Font size="22.0"/>
         </font>
      </Text>
      <Button mnemonicParsing="false" onAction="#forwardOneMonth" styleClass="btForwardAndBackOneMonth" text="&gt;"/>
