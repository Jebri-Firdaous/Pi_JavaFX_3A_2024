      <HBox prefHeight="100.0" prefWidth="175.0">
         <HBox spacing="26.5">
            <VBox>
               <Label text="Hour"/>
               <ComboBox fx:id="hourComboBox" prefHeight="31.0" prefWidth="64.0"/>
            </VBox>
            <VBox>
               <Label text="Minute"/>
               <ComboBox fx:id="minuteComboBox" prefHeight="38.0" prefWidth="96.0"/>
            </VBox>
         </HBox>
      </HBox>
   </VBox>
   <Button layoutX="75.0" layoutY="645.0" mnemonicParsing="false" onAction="#reserverBT" prefHeight="31.0"
           prefWidth="115.0" text="Réserver"/>

   <FlowPane/>

