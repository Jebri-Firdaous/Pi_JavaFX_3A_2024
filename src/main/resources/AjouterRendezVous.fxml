   <fx:include source="navBar.fxml"/>
   <VBox layoutX="75.0" layoutY="132.0" prefHeight="496.0" prefWidth="367.0" spacing="27.5">
      <VBox spacing="16.0">
         <VBox>
            <VBox spacing="11.0">
               <Label fx:id="labelClient" layoutX="516.0" layoutY="665.0" prefHeight="21.0"
                      prefWidth="104.0" text="Client"/>
               <ComboBox fx:id="comboboxClient" layoutX="561.0" layoutY="665.0"
                         maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"/>
            </VBox>
            <Label text="Specialite"/>
            <ComboBox fx:id="specialiteR" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                      prefHeight="18.0" prefWidth="344.0"/>
         </VBox>
      </VBox>
      <VBox>
